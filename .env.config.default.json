{
    "install_path": "E:\\dev",
    "arch_mapping": {
        "x86_64": {
            "java": "x64",
            "python": "amd64",
            "go": "amd64",
            "node": "x64",
            "rust": "x86_64-pc-windows-msvc",
            "maven": "x64",
            "gradle": "x64"
        },
        "aarch64": {
            "java": "aarch64",
            "python": "arm64",
            "go": "arm64",
            "node": "arm64",
            "rust": "aarch64-pc-windows-msvc",
            "maven": "arm64",
            "gradle": "arm64"
        }
    },
    "environments": [
        {
            "name": "java",
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Java开发环境",
            "repository": "https://corretto.aws/downloads/latest/amazon-corretto-%version%-%arch%-%platform%-jdk.%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "8",
                    "options": [
                        "8",
                        "21",
                        "17",
                        "11"
                    ],
                    "select_description": [
                        "非LTS",
                        "LTS",
                        "LTS",
                        "LTS",
                        "LTS"
                    ],
                    "description": "选择JDK版本"
                }
            ],
            "executable": [
                "%JAVA_HOME%",
                "bin"
            ],
            "environment": {
                "JAVA_HOME": "%INSTALL_DIR%"
            }
        },
        {
            "name": "python",
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Python开发环境",
            "repository": "https://www.python.org/ftp/python/%version%/python-%version%-%arch%.%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "3.12.1",
                    "options": [
                        "3.12.1",
                        "3.11.7",
                        "3.10.11",
                        "3.9.13"
                    ],
                    "select_description": [
                        "最新版",
                        "稳定版",
                        "旧版",
                        "旧版"
                    ],
                    "description": "选择Python版本"
                },
                {
                    "name": "pip",
                    "type": "select",
                    "default": "true",
                    "options": [
                        "true",
                        "false"
                    ],
                    "description": "是否安装pip"
                }
            ],
            "executable": [
                "%PYTHON_HOME%",
                "bin"
            ],
            "environment": {
                "PYTHON_HOME": "%INSTALL_DIR%"
            }
        },
        {
            "name": "go",
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Go开发环境",
            "repository": "https://golang.google.cn/dl/go%version%.%platform%-%arch%.%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "1.24",
                    "options": [
                        "1.24",
                        "1.23.4",
                        "1.23.3"
                    ],
                    "select_description": [
                        "最新版",
                        "稳定版",
                        "旧版"
                    ],
                    "description": "选择Go版本"
                }
            ],
            "executable": [
                "%GOROOT%",
                "bin"
            ],
            "environment": {
                "GOROOT": "%INSTALL_DIR%",
                "GOPATH": "%INSTALL_DIR%/workspace"
            }
        },
        {
            "name": "node",
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Node.js开发环境",
            "repository": "https://nodejs.org/dist/v%version%/node-v%version%-%platform%-%arch%.%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "22.12.0",
                    "options": [
                        "22.12.0",
                        "20.18.1",
                        "18.20.5"
                    ],
                    "select_description": [
                        "最新LTS",
                        "稳定LTS",
                        "旧版LTS"
                    ],
                    "description": "选择Node.js版本"
                }
            ],
            "executable": [
                "%NODE_HOME%"
            ],
            "environment": {
                "NODE_HOME": "%INSTALL_DIR%"
            }
        },
        {
            "name": "rust",
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Rust开发环境",
            "repository": "https://static.rust-lang.org/rustup/dist/%platform%-%arch%/rustup-init%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "stable",
                    "options": [
                        "stable",
                        "beta",
                        "nightly"
                    ],
                    "select_description": [
                        "稳定版",
                        "测试版",
                        "每日构建版"
                    ],
                    "description": "选择Rust版本"
                }
            ],
            "executable": [
                "%RUSTUP_HOME%",
                "bin"
            ],
            "environment": {
                "RUSTUP_HOME": "%INSTALL_DIR%",
                "CARGO_HOME": "%INSTALL_DIR%"
            }
        },
        {
            "name": "maven",
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Maven构建工具",
            "repository": "https://dlcdn.apache.org/maven/maven-3/%version%/binaries/apache-maven-%version%-bin.%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "3.9.6",
                    "options": [
                        "3.9.6",
                        "3.8.8",
                        "3.6.3"
                    ],
                    "select_description": [
                        "最新版",
                        "稳定版",
                        "旧版"
                    ],
                    "description": "选择Maven版本"
                }
            ],
            "executable": [
                "%MAVEN_HOME%",
                "bin"
            ],
            "environment": {
                "MAVEN_HOME": "%INSTALL_DIR%"
            }
        },
        {
            "name": "gradle",
            "format": {
                "windows": "zip",
                "macos": "zip",
                "linux": "zip"
            },
            "description": "Gradle构建工具",
            "repository": "https://services.gradle.org/distributions/gradle-%version%-bin.%format%",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "8.5",
                    "options": [
                        "8.5",
                        "8.4",
                        "7.6.3"
                    ],
                    "select_description": [
                        "最新版",
                        "稳定版",
                        "旧版"
                    ],
                    "description": "选择Gradle版本"
                }
            ],
            "executable": [
                "%GRADLE_HOME%",
                "bin"
            ],
            "environment": {
                "GRADLE_HOME": "%INSTALL_DIR%"
            }
        }
    ]
}