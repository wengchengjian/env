{
    "install_path": "E:\\dev",
    "environments": [
        {
            "name": "Java",
            "args": [
                {
                    "name": "version",
                    "type": "select",
                    "default": "17",
                    "options": ["23", "21", "17", "11", "8"],
                    "select_description": ["非LTS", "LTS", "LTS", "LTS", "LTS"],
                    "description": "选择JDK版本"
                }
            ],
            "executable": ["%JAVA_HOME%", "bin"],

            "environment": {
                "JAVA_HOME": "%INSTALL_DIR%"
            },
            "format": {
                "windows": "zip",
                "macos": "tar.gz",
                "linux": "tar.gz"
            },
            "description": "Java开发环境",
            "repository": "https://corretto.aws/downloads/latest/amazon-corretto-%version%-%arch%-%platform%-jdk.%format%"
        }
    ]
}